  gem 'sass-rails'
  gem 'coffee-rails'
  gem 'uglifier'
  gem 'modernizr-rails'
  gem 'execjs'
  gem 'therubyracer'
  gem 'jquery-rails'
  gem 'coffee-script'
  gem 'haml'
  gem 'redcarpet'
  gem 'bootstrap-sass'
end

gem 'unicorn'

gem 'geocoder'

gem 'sqlite3'

gem 'iconv'

gem 'actionpack-action_caching'
